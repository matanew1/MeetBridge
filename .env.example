# Firebase Configuration
# [SECURITY] Copy this file to .env and replace with your actual values
# Note: Firebase API keys in frontend are safe per Firebase documentation,
# but keeping them in .env allows easy rotation and prevents accidental exposure
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_DATABASE_URL=https://your_project-default-rtdb.firebaseio.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id

# FCM Configuration
EXPO_PUBLIC_FCM_SERVER_KEY=your_fcm_server_key

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Cloudinary Configuration
# [SECURITY] Never expose API secret in frontend - use signed uploads
EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name
EXPO_PUBLIC_CLOUDINARY_UPLOAD_PRESET=your_upload_preset
EXPO_PUBLIC_CLOUDINARY_API_KEY=your_api_key

# Application Configuration
EXPO_PUBLIC_APP_ENV=development

{
    "rules": {
        "presence": {
            "$uid": {
                ".read": "auth != null",
                ".write": "auth != null && $uid === auth.uid",
                ".validate": "newData.hasChildren(['status', 'lastSeen', 'updatedAt']) && newData.child('status').isString() && newData.child('lastSeen').isNumber() && newData.child('updatedAt').isNumber()"
            }
        },
        ".read": false,
        ".write": false
    }
}